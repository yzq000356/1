function calculator(num1, operator, num2)
{
    switch (operator) {
        case '+':
            return num1 + num2;
        case '-':
            return num1 - num2;
        case '*':
            return num1 * num2;
        case '/':
            if (num2 === 0) {
                return "除数不能为0";
            }
            return num1 / num2;
        default:
            return "不支持的操作符";
    }
}

